# If set, backend uses OpenAI-compatible provider; otherwise uses deterministic MockAiProvider.
# Use a real key only in your local .env (never commit it).
OPENAI_API_KEY=
# Optional (defaults to https://api.openai.com/v1)
OPENAI_BASE_URL=
# Optional model name
OPENAI_MODEL=

PORT=3001
LOG_LEVEL=info

# Primary database (PostgreSQL)
#
# Local:
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/interview_orchestrator
#
# Supabase:
# Use the Postgres connection string from Supabase settings.
# Either set DATABASE_URL or SUPABASE_DB_URL / SUPABASE_DATABASE_URL.
# Example (Supabase): postgresql://postgres:<password>@db.<project-ref>.supabase.co:5432/postgres
DATABASE_URL=

# Supabase Postgres typically requires TLS.
# Set to `require` for Supabase.
PGSSLMODE=require

# Session/state store (Redis) for live interviews
# Example (local): redis://localhost:6379
REDIS_URL=
